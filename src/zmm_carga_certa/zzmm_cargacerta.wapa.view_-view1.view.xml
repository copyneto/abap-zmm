<mvc:View controllerName="br.com.trescoracoes.pcargacerta.controller.View1"                                                                                                                                                                                    
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"                                                                                                                                                                                                            
    xmlns="sap.m" xmlns:core="sap.ui.core">                                                                                                                                                                                                                    
    <Page id="page" title="{i18n>title}">                                                                                                                                                                                                                      
        <content>                                                                                                                                                                                                                                              
            <VBox>                                                                                                                                                                                                                                             
                <Label text="Executar ZMMSD_PLANLOG"></Label>                                                                                                                                                                                                  
                <HBox alignItems="Center">                                                                                                                                                                                                                     
                    <Label text="Report:" width="105px"></Label>                                                                                                                                                                                               
                <!-- <ComboBox id="InputTran" width="300px" selectionChange="onSelectionChange" items="{/ZC_MM_PLANLOG_VARIANTS}">                                                                                                                             
                    <items>                                                                                                                                                                                                                                    
                        <core:ListItem text="{report}" key="{report}"></core:ListItem>                                                                                                                                                                         
                    </items>                                                                                                                                                                                                                                   
                </ComboBox> -->                                                                                                                                                                                                                                
                <ComboBox id="InputTran" width="300px" selectionChange="onSelectionChange">                                                                                                                                                                    
                    <items>                                                                                                                                                                                                                                    
                        <core:Item text="ZTRANICMSNFE" key="ZTRANSICMS_A"></core:Item>                                                                                                                                                                         
                        <core:Item text="MB52" key="MB52"></core:Item>                                                                                                                                                                                         
                        <core:Item text="ZWMSCONFPROD" key="ZWMSCONFPROD"></core:Item>                                                                                                                                                                         
                        <core:Item text="ZSDR319" key="ZSDR319"></core:Item>                                                                                                                                                                                   
                    </items>                                                                                                                                                                                                                                   
                </ComboBox>                                                                                                                                                                                                                                    
                </HBox>                                                                                                                                                                                                                                        
                <HBox alignItems="Center">                                                                                                                                                                                                                     
                    <Label text="Variante:" width="105px"></Label>                                                                                                                                                                                             
                    <ComboBox id="InputVari"  loadItems="onLoadVariant" width="300px" enabled="false" items="{/ZC_MM_PLANLOG_VARIANTS}">                                                                                                                       
                        <items>                                                                                                                                                                                                                                
                            <core:ListItem text="{variant}" key="{variant}" ></core:ListItem>                                                                                                                                                                  
                        </items>                                                                                                                                                                                                                               
                    </ComboBox>                                                                                                                                                                                                                                
                </HBox>                                                                                                                                                                                                                                        
            </VBox>                                                                                                                                                                                                                                            
            <VBox>                                                                                                                                                                                                                                             
            <Button id="ButtonExecutar" text="Executar" press="onExecutarPress"/>                                                                                                                                                                              
        </VBox>                                                                                                                                                                                                                                                
                                                                                                                                                                                                                                                               
</content>                                                                                                                                                                                                                                                     
    </Page>                                                                                                                                                                                                                                                    
</mvc:View>                                                                                                                                                                                                                                                    