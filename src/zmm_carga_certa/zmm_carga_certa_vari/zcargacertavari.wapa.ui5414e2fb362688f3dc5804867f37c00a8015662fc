        onBeforeRendering: function() {                                                                                                                                                                                                                        
            debugger ;this.byId("tableCreate").setModel(this.jModel);                                                                                                                                                                                          
            this.byId("tableCreateE").setModel(this.jModelE)                                                                                                                                                                                                   
        },                                                                                                                                                                                                                                                     
        addRow: function(e) {                                                                                                                                                                                                                                  
            debugger ;var t = this.getView().byId("tableCreate");                                                                                                                                                                                              
            if (this._data.VarianteAdd[0]) {                                                                                                                                                                                                                   
                if (this._data.VarianteAdd[0]["Campo"] === "") {                                                                                                                                                                                               
                    this._data.VarianteAdd = []                                                                                                                                                                                                                
                }                                                                                                                                                                                                                                              
            }                                                                                                                                                                                                                                                  
            this._data.VarianteAdd.push({                                                                                                                                                                                                                      
                Campo: this.getView().byId("inputCampo_").getSelectedKey(),                                                                                                                                                                                    
                Opti: this.getView().byId("inputOpti_").getValue(),                                                                                                                                                                                            
                Low: this.getView().byId("inputLow_").getValue(),                                                                                                                                                                                              
                High: this.getView().byId("inputHigh_").getValue()                                                                                                                                                                                             
            });                                                                                                                                                                                                                                                
            this.jModel.refresh();                                                                                                                                                                                                                             
            t.getModel().refresh()                                                                                                                                                                                                                             
        },                                                                                                                                                                                                                                                     
        addRowE: function(e) {                                                                                                                                                                                                                                 
            debugger ;var t = this.getView().byId("tableEdit");                                                                                                                                                                                                
            if (this._dataE.VarianteAdd[0]) {                                                                                                                                                                                                                  
                if (this._dataE.VarianteAdd[0]["Campo"] === "") {                                                                                                                                                                                              
                    this._dataE.VarianteAdd = []                                                                                                                                                                                                               
                }                                                                                                                                                                                                                                              
            }                                                                                                                                                                                                                                                  
            var i = t.getItems();                                                                                                                                                                                                                              
            var r = i[1];                                                                                                                                                                                                                                      
            this._dataE.VarianteAdd.push({                                                                                                                                                                                                                     
                Campo: this.getView().byId("inputCampoE_").getSelectedKey(),                                                                                                                                                                                   
                Opti: this.getView().byId("inputOptiE_").getValue(),                                                                                                                                                                                           
                Low: this.getView().byId("inputLowE_").getValue(),                                                                                                                                                                                             
                High: this.getView().byId("inputHighE_").getValue()                                                                                                                                                                                            
            });                                                                                                                                                                                                                                                
            this.jModelE.refresh();                                                                                                                                                                                                                            
            t.getModel().refresh()                                                                                                                                                                                                                             
        },                                                                                                                                                                                                                                                     
        deleteRow: function(e) {                                                                                                                                                                                                                               
            debugger ;var t = e.getSource().getBindingContext().getObject();                                                                                                                                                                                   
            for (var i = 0; i < this._data.VarianteAdd.length; i++) {                                                                                                                                                                                          
                if (this._data.VarianteAdd[i] == t) {                                                                                                                                                                                                          
                    this._data.VarianteAdd.splice(i, 1);                                                                                                                                                                                                       
                    this.jModel.refresh();                                                                                                                                                                                                                     
                    break                                                                                                                                                                                                                                      
                }                                                                                                                                                                                                                                              
            }                                                                                                                                                                                                                                                  
        },                                                                                                                                                                                                                                                     
        deleteRowE: function(e) {                                                                                                                                                                                                                              
            debugger ;var t = e.getSource().getBindingContext().getObject();                                                                                                                                                                                   
            for (var i = 0; i < this._dataE.VarianteAdd.length; i++) {                                                                                                                                                                                         
                if (this._dataE.VarianteAdd[i] == t) {                                                                                                                                                                                                         
                    this._dataE.VarianteAdd.splice(i, 1);                                                                                                                                                                                                      
                    this.jModelE.refresh();                                                                                                                                                                                                                    
                    break                                                                                                                                                                                                                                      
                }                                                                                                                                                                                                                                              
            }                                                                                                                                                                                                                                                  
        },                                                                                                                                                                                                                                                     
        deletaRegistro: function(e) {                                                                                                                                                                                                                          
            debugger ;var t = this.getView().getModel();                                                                                                                                                                                                       
            var i, r, a, o, n, l, g;                                                                                                                                                                                                                           
            i = e.getSource().getBindingContext().getObject().Report;                                                                                                                                                                                          
            r = e.getSource().getBindingContext().getObject().Vari;                                                                                                                                                                                            
            a = e.getSource().getBindingContext().getObject().Opti;                                                                                                                                                                                            
            l = e.getSource().getBindingContext().getObject().Field;                                                                                                                                                                                           
            o = e.getSource().getBindingContext().getObject().Low;                                                                                                                                                                                             
            n = e.getSource().getBindingContext().getObject().High;                                                                                                                                                                                            
            g = e.getSource().getBindingContext().getObject().Cont;                                                                                                                                                                                            
            var c = {                                                                                                                                                                                                                                          
                Report: i,                                                                                                                                                                                                                                     
                Vari: r,                                                                                                                                                                                                                                       
                Cont: g                                                                                                                                                                                                                                        
            };                                                                                                                                                                                                                                                 
            var u = this;                                                                                                                                                                                                                                      
            var d = "/Variante";                                                                                                                                                                                                                               
            var h = {                                                                                                                                                                                                                                          
                success: function(e, t) {                                                                                                                                                                                                                      
                    debugger ;jQuery.sap.require("sap.m.MessageBox");                                                                                                                                                                                          
                    s.show("Registros removidos com sucesso.", {                                                                                                                                                                                               
                        icon: sap.m.MessageBox.Icon.SUCCESS,                                                                                                                                                                                                   
                        title: "Informação",                                                                                                                                                                                                                   
                        actions: sap.m.MessageBox.Action.OK,                                                                                                                                                                                                   
                        styleClass: "",                                                                                                                                                                                                                        
                        initialFocus: null,                                                                                                                                                                                                                    
                        textDirection: sap.ui.core.TextDirection.Inherit,                                                                                                                                                                                      
                        verticalScrolling: true,                                                                                                                                                                                                               
                        horizontalScrolling: true                                                                                                                                                                                                              
                    })                                                                                                                                                                                                                                         
                },                                                                                                                                                                                                                                             
                error: function(e) {                                                                                                                                                                                                                           
                    debugger ;var t = JSON.parse(e.responseText);                                                                                                                                                                                              
                    var i = t.error.innererror.errordetails;                                                                                                                                                                                                   
                    s.show("Falha ao remover Registros" + t.error.message.value, {                                                                                                                                                                             
                        icon: sap.m.MessageBox.Icon.ERROR,                                                                                                                                                                                                     
                        title: "Informação",                                                                                                                                                                                                                   
                        actions: sap.m.MessageBox.Action.OK,                                                                                                                                                                                                   
                        styleClass: "",                                                                                                                                                                                                                        
                        initialFocus: null,                                                                                                                                                                                                                    
                        textDirection: sap.ui.core.TextDirection.Inherit,                                                                                                                                                                                      
                        verticalScrolling: true,                                                                                                                                                                                                               
                        horizontalScrolling: true                                                                                                                                                                                                              
                    })                                                                                                                                                                                                                                         
                }                                                                                                                                                                                                                                              
            };                                                                                                                                                                                                                                                 
            t.remove("/Variante(Report='" + c.Report + "',Vari='" + c.Vari + "',Cont='" + c.Cont + "')", {                                                                                                                                                     
                success: $.proxy(function(e) {                                                                                                                                                                                                                 
                    debugger ;jQuery.sap.require("sap.m.MessageBox");                                                                                                                                                                                          
                    sap.m.MessageBox.show("Visita removida com sucesso!", {                                                                                                                                                                                    
                        icon: sap.m.MessageBox.Icon.SUCCESS,                                                                                                                                                                                                   
                        title: "Informação",                                                                                                                                                                                                                   
                        actions: sap.m.MessageBox.Action.OK,                                                                                                                                                                                                   
                        onClose: function(e) {},                                                                                                                                                                                                               
                        styleClass: "",                                                                                                                                                                                                                        
                        initialFocus: null,                                                                                                                                                                                                                    
                        textDirection: sap.ui.core.TextDirection.Inherit,                                                                                                                                                                                      
                        verticalScrolling: true,                                                                                                                                                                                                               
                        horizontalScrolling: true                                                                                                                                                                                                              
                    })                                                                                                                                                                                                                                         
                }, this),                                                                                                                                                                                                                                      
                error: $.proxy(function(e) {                                                                                                                                                                                                                   
                    debugger ;oResponses.errors.push(e);                                                                                                                                                                                                       
                    jQuery.sap.require("sap.m.MessageBox");                                                                                                                                                                                                    
                    sap.m.MessageBox.show(JSON.parse(e.responseText).error.message.value, {                                                                                                                                                                    
                        icon: sap.m.MessageBox.Icon.ERROR,                                                                                                                                                                                                     
                        title: "Informação",                                                                                                                                                                                                                   
                        actions: sap.m.MessageBox.Action.OK,                                                                                                                                                                                                   
                        onClose: null,                                                                                                                                                                                                                         
                        styleClass: "",                                                                                                                                                                                                                        
                        initialFocus: null,                                                                                                                                                                                                                    
                        textDirection: sap.ui.core.TextDirection.Inherit,                                                                                                                                                                                      
                        verticalScrolling: true,                                                                                                                                                                                                               
                        horizontalScrolling: true                                                                                                                                                                                                              
                    })                                                                                                                                                                                                                                         
                }, this),                                                                                                                                                                                                                                      
                aSync: true                                                                                                                                                                                                                                    
            })                                                                                                                                                                                                                                                 
        },                                                                                                                                                                                                                                                     
        filterVariante: function(e) {                                                                                                                                                                                                                          
            var t = [];                                                                                                                                                                                                                                        
            var i = this.byId("filterReport").getValue();                                                                                                                                                                                                      
            var s = this.byId("filterVari").getValue();                                                                                                                                                                                                        
            debugger ;if (i) {                                                                                                                                                                                                                                 
                t.push(new r("Report",a.EQ,i))                                                                                                                                                                                                                 
            }                                                                                                                                                                                                                                                  
            if (s) {                                                                                                                                                                                                                                           
                t.push(new r("Vari",a.EQ,s))                                                                                                                                                                                                                   
            }                                                                                                                                                                                                                                                  
            var o = this.getView().byId("tableVariante");                                                                                                                                                                                                      
            var n = o.getBinding("rows");                                                                                                                                                                                                                      
            n.filter(t);                                                                                                                                                                                                                                       
            var o = this.getView().byId("tableEdit");                                                                                                                                                                                                          
            var n = o.getBinding("items");                                                                                                                                                                                                                     
            n.filter(t)                                                                                                                                                                                                                                        
        },                                                                                                                                                                                                                                                     
        onChangeCampoU: function(e) {                                                                                                                                                                                                                          
            debugger ;var t = e.getParameter("value");                                                                                                                                                                                                         
            var i;                                                                                                                                                                                                                                             
            i = e.getSource().getBindingContext();                                                                                                                                                                                                             
            var s = this.getView().byId("tableCreate");                                                                                                                                                                                                        
            var o = this.getView().byId("inputCampo");                                                                                                                                                                                                         
            var n = [];                                                                                                                                                                                                                                        
            var l = this.getView().byId("filterReport").getValue();                                                                                                                                                                                            
            n.push(new r("Report",a.Contains,l));                                                                                                                                                                                                              
            var g = i.getModel();                                                                                                                                                                                                                              
            o.getSuggestionItems().filter(n);                                                                                                                                                                                                                  
            s.getModel().refresh();                                                                                                                                                                                                                            
            selected_nome = 0;                                                                                                                                                                                                                                 
            selected_nome = e.getParameter("selectedItem");                                                                                                                                                                                                    
            if (selected_nome) {                                                                                                                                                                                                                               
                debugger ;var c = e.getParameter("selectedItem").getKey()                                                                                                                                                                                      
            }                                                                                                                                                                                                                                                  
        },                                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
        onLoadVariant: function(e) {                                                                                                                                                                                                                           
            debugger;                                                                                                                                                                                                                                          
            var aFilters = [];                                                                                                                                                                                                                                 
            var t = this.getView().byId("filterReport").getSelectedKey();                                                                                                                                                                                      
            aFilters.push(new sap.ui.model.Filter("Report", sap.ui.model.FilterOperator.EQ, t));                                                                                                                                                               
            e.getSource().getBinding("items").filter(aFilters, sap.ui.model.FilterType.Application)                                                                                                                                                            
        },                                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
        handleSuggest: function(e) {                                                                                                                                                                                                                           
            debugger ;var t = this.getView().byId("filterReport").getValue();                                                                                                                                                                                  
            var i = [];                                                                                                                                                                                                                                        
            if (t) {                                                                                                                                                                                                                                           
                i.push(new r("Report",sap.ui.model.FilterOperator.StartsWith,t))                                                                                                                                                                               
            }                                                                                                                                                                                                                                                  
            e.getSource().getBinding("suggestionItems").filter(i)                                                                                                                                                                                              
        },                                                                                                                                                                                                                                                     
        rebindTable: function(e, t) {                                                                                                                                                                                                                          
            this.oTable.bindItems({                                                                                                                                                                                                                            
                path: "/Variante",                                                                                                                                                                                                                             
                template: e,                                                                                                                                                                                                                                   
                templateShareable: true,                                                                                                                                                                                                                       
                key: "Campo"                                                                                                                                                                                                                                   
            }).setKeyboardMode(t)                                                                                                                                                                                                                              
        },                                                                                                                                                                                                                                                     
        onEdit: function() {                                                                                                                                                                                                                                   
            this.aProductCollection = deepExtend([], this.oModel.getProperty("/Variante"));                                                                                                                                                                    
            this.byId("editButton").setVisible(false);                                                                                                                                                                                                         
            this.byId("saveButton").setVisible(true);                                                                                                                                                                                                          
            this.byId("cancelButton").setVisible(true);                                                                                                                                                                                                        
            this.rebindTable(this.oEditableTemplate, "Edit")                                                                                                                                                                                                   
        },                                                                                                                                                                                                                                                     
        _formFragments: {},                                                                                                                                                                                                                                    
        _getFormFragment: function(e) {                                                                                                                                                                                                                        
            var t = this._formFragments[e];                                                                                                                                                                                                                    
            if (t) {                                                                                                                                                                                                                                           
                return t                                                                                                                                                                                                                                       
            }                                                                                                                                                                                                                                                  
            t = sap.ui.xmlfragment(this.getView().getId(), "webapp.view." + e);                                                                                                                                                                                
            var i = this._formFragments[e] = t;                                                                                                                                                                                                                
            return i                                                                                                                                                                                                                                           
        },                                                                                                                                                                                                                                                     
        _showFormFragment: function(e) {                                                                                                                                                                                                                       
            var t = this.getView().byId("page");                                                                                                                                                                                                               
            t.removeAllContent();                                                                                                                                                                                                                              
            t.insertContent(this._getFormFragment(e))                                                                                                                                                                                                          
        },                                                                                                                                                                                                                                                     
        _clearAfterSave: function() {                                                                                                                                                                                                                          
            this.getView().byId("filterReport").setValue("");                                                                                                                                                                                                  
            this.getView().byId("filterVari").setValue("")                                                                                                                                                                                                     
        }                                                                                                                                                                                                                                                      
    })                                                                                                                                                                                                                                                         
});                                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               