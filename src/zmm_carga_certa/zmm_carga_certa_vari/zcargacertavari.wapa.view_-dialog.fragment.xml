<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:grid="sap.ui.layout.cssgrid" xmlns:core="sap.ui.core" xmlns:form="sap.ui.layout.form" xmlns:u="sap.ui.unified" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/+
1">                                                                                                                                                                                                                                                            
    <Dialog title="Nova Programação de Visita" class="sapUiResponsiveContentPadding" icon="sap-icon://create-form" afterClose="onDialogCloseButton">                                                                                                           
        <content>                                                                                                                                                                                                                                              
            <HBox alignItems="Center">                                                                                                                                                                                                                         
                <Label text="Cliente" width="130px" required="true" labelFor="cliente" />                                                                                                                                                                      
                <Input id="cliente" width="150px" required="true" submit="onClienteChange" change="onClienteChange" valueHelpRequest="createContent" />                                                                                                        
                <Label text="CNPJ" width="90px" textAlign="Center" />                                                                                                                                                                                          
                <Input id="cnpj" width="160px" enabled="false" />                                                                                                                                                                                              
            </HBox>                                                                                                                                                                                                                                            
            <HBox alignItems="Center">                                                                                                                                                                                                                         
                <Label text="Razão Social" width="130px" />                                                                                                                                                                                                    
                <Input id="razaosocial" value="{Nome}" width="460px" liveChange=".searchNome" showSuggestion="true" suggestionItemSelected="setKunnr" suggestionItems="{/BuscaClienteNome}">                                                                   
                    <suggestionItems>                                                                                                                                                                                                                          
                        <core:Item key="{Cliente}-{Cidade}-{Estado}-{Consultor}-{Nome}-{Cnpj}" text="{RazaoSocial}" />                                                                                                                                         
                    </suggestionItems>                                                                                                                                                                                                                         
                </Input>                                                                                                                                                                                                                                       
            </HBox>                                                                                                                                                                                                                                            
            <HBox alignItems="Center">                                                                                                                                                                                                                         
                <Label text="Cidade" width="130px" />                                                                                                                                                                                                          
                <Input id="cidade" width="150px" enabled="false" />                                                                                                                                                                                            
                <Label text="Estado" width="90px" textAlign="Center" />                                                                                                                                                                                        
                <Input id="estado" width="120px" enabled="false" />                                                                                                                                                                                            
            </HBox>                                                                                                                                                                                                                                            
            <HBox alignItems="Center">                                                                                                                                                                                                                         
                <Label text="Data da última visita" width="130px" />                                                                                                                                                                                           
                <Input id="dataultimavisita" width="120px" enabled="false" />                                                                                                                                                                                  
            </HBox>                                                                                                                                                                                                                                            
            <HBox alignItems="Center">                                                                                                                                                                                                                         
                <Label text="Consultor" width="130px" required="true" labelFor="consultor" />                                                                                                                                                                  
                <Input id="consultor" width="120px" required="true" change="onConsultorChange" submit="onConsultorChange" valueHelpRequest="ValueConsultorRequest" />                                                                                          
            </HBox>                                                                                                                                                                                                                                            
            <HBox alignItems="Center">                                                                                                                                                                                                                         
                <Label text="Nome Consultor" width="130px" labelFor="nomeConsultor" />                                                                                                                                                                         
                <Input id="nomeConsultor" value="{teste}" width="460px" liveChange=".searchNomeC" showSuggestion="true" suggestionItemSelected="setCodigo" suggestionItems="{/BuscaConsultorNome}" change="onNomeConsultorChange">                             
                    <suggestionItems>                                                                                                                                                                                                                          
                        <core:Item key="{Codigo}" text="{Nome}" />                                                                                                                                                                                             
                    </suggestionItems>                                                                                                                                                                                                                         
                </Input>                                                                                                                                                                                                                                       
            </HBox>                                                                                                                                                                                                                                            
            <HBox alignItems="Center">                                                                                                                                                                                                                         
                <Label text="Status" width="130px" required="true" />                                                                                                                                                                                          
                <ComboBox id="status" width="200px" selectedKey="0" class="sapUiContentPadding" enabled="false" required="true">                                                                                                                               
                    <items name="teste">                                                                                                                                                                                                                       
                        <core:Item key="0" text="Pendente" tooltip="Pendente" />                                                                                                                                                                               
                        <core:Item key="1" text="Em Andamento" tooltip="Em Andamento" />                                                                                                                                                                       
                        <core:Item key="2" text="Concluído" tooltip="Concluído" />                                                                                                                                                                             
                        <core:Item key="3" text="Cancelado" tooltip="Cancelado" />                                                                                                                                                                             
                    </items>                                                                                                                                                                                                                                   
                </ComboBox>                                                                                                                                                                                                                                    
            </HBox>                                                                                                                                                                                                                                            
            <HBox>                                                                                                                                                                                                                                             
                <Button id="buttonSalvar" type="Accept" text="Salvar" press="OnSalvar" class="sapUiSmallMarginEnd" />                                                                                                                                          
                <Label width="10px" />                                                                                                                                                                                                                         
                <Button id="btnCancelar" type="Reject" text="Cancelar" press="onDialogCloseButton" class="sapUiSmallMarginEnd" />                                                                                                                              
                <Label width="250px" />                                                                                                                                                                                                                        
                <u:FileUploader id="FileUploaderId" buttonText="Carga via Excel" text="Carga via Excel" sameFilenameAllowed="true" iconOnly="false" buttonOnly="true" fileType="XLSX,xlsx" icon="sap-icon://upload" iconFirst="true" style="Emphasized" change+
="onUpload" />                                                                                                                                                                                                                                                 
            </HBox>                                                                                                                                                                                                                                            
            <Table id="tableExcel" items="{localModel>/items}" visible="false">                                                                                                                                                                                
                <columns>                                                                                                                                                                                                                                      
                    <Column>                                                                                                                                                                                                                                   
                        <Label text="Cliente" />                                                                                                                                                                                                               
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Label text="Consultor" />                                                                                                                                                                                                             
                    </Column>                                                                                                                                                                                                                                  
                </columns>                                                                                                                                                                                                                                     
                <items>                                                                                                                                                                                                                                        
                    <ColumnListItem>                                                                                                                                                                                                                           
                        <cells>                                                                                                                                                                                                                                
                            <Text text="{localModel>cliente}" />                                                                                                                                                                                               
                            <Text text="{localModel>consultor}" />                                                                                                                                                                                             
                        </cells>                                                                                                                                                                                                                               
                    </ColumnListItem>                                                                                                                                                                                                                          
                </items>                                                                                                                                                                                                                                       
            </Table>                                                                                                                                                                                                                                           
        </content>                                                                                                                                                                                                                                             
    </Dialog>                                                                                                                                                                                                                                                  
</core:FragmentDefinition>                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               