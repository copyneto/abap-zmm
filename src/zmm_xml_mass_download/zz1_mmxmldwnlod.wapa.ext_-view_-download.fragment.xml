<c:FragmentDefinition                                                                                                                                                                                                                                          
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:c="sap.ui.core"                                                                                                                                                                                                                                         
	xmlns:u="sap.ui.unified"                                                                                                                                                                                                                                      
>                                                                                                                                                                                                                                                              
	<Dialog                                                                                                                                                                                                                                                       
		id="downloadDialog"                                                                                                                                                                                                                                          
		icon="sap-icon://download"                                                                                                                                                                                                                                   
		title="{i18n>DialogDownloadTitle}"                                                                                                                                                                                                                           
	>                                                                                                                                                                                                                                                             
		<VBox                                                                                                                                                                                                                                                        
			justifyContent="SpaceBetween"                                                                                                                                                                                                                               
			alignContent="SpaceAround"                                                                                                                                                                                                                                  
			alignItems="Inherit"                                                                                                                                                                                                                                        
			fitContainer="true"                                                                                                                                                                                                                                         
		>                                                                                                                                                                                                                                                            
			<Panel                                                                                                                                                                                                                                                      
				headerText="{i18n>rbOptions}"                                                                                                                                                                                                                              
				expanded="true"                                                                                                                                                                                                                                            
			>                                                                                                                                                                                                                                                           
				<HBox                                                                                                                                                                                                                                                      
					id="pnlOptions"                                                                                                                                                                                                                                           
					justifyContent="SpaceAround"                                                                                                                                                                                                                              
					alignContent="Inherit"                                                                                                                                                                                                                                    
					fitContainer="true"                                                                                                                                                                                                                                       
				>                                                                                                                                                                                                                                                          
					<CheckBox                                                                                                                                                                                                                                                 
						id="cbDanfe"                                                                                                                                                                                                                                             
						selected="true"                                                                                                                                                                                                                                          
						text="{i18n>cbDanfe}"                                                                                                                                                                                                                                    
					/>                                                                                                                                                                                                                                                        
					<CheckBox                                                                                                                                                                                                                                                 
						id="cbCte"                                                                                                                                                                                                                                               
						text="{i18n>cbCte}"                                                                                                                                                                                                                                      
					/>                                                                                                                                                                                                                                                        
				</HBox>                                                                                                                                                                                                                                                    
			</Panel>                                                                                                                                                                                                                                                    
			<Panel                                                                                                                                                                                                                                                      
				headerText="{i18n>rbDownload}"                                                                                                                                                                                                                             
				expanded="true"                                                                                                                                                                                                                                            
			>                                                                                                                                                                                                                                                           
				<VBox id="pnlDownload">                                                                                                                                                                                                                                    
					<HBox                                                                                                                                                                                                                                                     
						justifyContent="SpaceBetween"                                                                                                                                                                                                                            
						alignContent="Center"                                                                                                                                                                                                                                    
						fitContainer="true"                                                                                                                                                                                                                                      
					>                                                                                                                                                                                                                                                         
						<RadioButtonGroup                                                                                                                                                                                                                                        
							select=".onShowInput"                                                                                                                                                                                                                                   
							id="rbOptions"                                                                                                                                                                                                                                          
							width="100%"                                                                                                                                                                                                                                            
							columns="2"                                                                                                                                                                                                                                             
						>                                                                                                                                                                                                                                                        
							<RadioButton                                                                                                                                                                                                                                            
								id="rbLocal"                                                                                                                                                                                                                                           
								selected="true"                                                                                                                                                                                                                                        
								text="{i18n>rbLocal}"                                                                                                                                                                                                                                  
							/>                                                                                                                                                                                                                                                      
							<RadioButton                                                                                                                                                                                                                                            
								id="rbServer"                                                                                                                                                                                                                                          
								text="{i18n>rbServer}"                                                                                                                                                                                                                                 
							/>                                                                                                                                                                                                                                                      
						</RadioButtonGroup>                                                                                                                                                                                                                                      
					</HBox>                                                                                                                                                                                                                                                   
					<VBox                                                                                                                                                                                                                                                     
						id="fup"                                                                                                                                                                                                                                                 
						visible="false"                                                                                                                                                                                                                                          
					>                                                                                                                                                                                                                                                         
						<Label                                                                                                                                                                                                                                                   
							text="{i18n>File}"                                                                                                                                                                                                                                      
							labelFor="fup"                                                                                                                                                                                                                                          
						/>                                                                                                                                                                                                                                                       
						<Input                                                                                                                                                                                                                                                   
							id="path"                                                                                                                                                                                                                                               
							placeholder="{i18n>FilePlaceholder}"                                                                                                                                                                                                                    
						/>                                                                                                                                                                                                                                                       
					</VBox>                                                                                                                                                                                                                                                   
				</VBox>                                                                                                                                                                                                                                                    
			</Panel>                                                                                                                                                                                                                                                    
		</VBox>                                                                                                                                                                                                                                                      
		<beginButton>                                                                                                                                                                                                                                                
			<Button                                                                                                                                                                                                                                                     
				text="{i18n>ButtonDialogAccept}"                                                                                                                                                                                                                           
				press=".onAccept"                                                                                                                                                                                                                                          
				type="Accept"                                                                                                                                                                                                                                              
			/>                                                                                                                                                                                                                                                          
		</beginButton>                                                                                                                                                                                                                                               
		<endButton>                                                                                                                                                                                                                                                  
			<Button                                                                                                                                                                                                                                                     
				text="{i18n>ButtonDialogClose}"                                                                                                                                                                                                                            
				press=".onClose"                                                                                                                                                                                                                                           
				type="Reject"                                                                                                                                                                                                                                              
			/>                                                                                                                                                                                                                                                          
		</endButton>                                                                                                                                                                                                                                                 
	</Dialog>                                                                                                                                                                                                                                                     
</c:FragmentDefinition>                                                                                                                                                                                                                                        