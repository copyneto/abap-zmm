@EndUserText.label: 'CDS de Autorização'
@MappingRole: true
define role ZI_MM_EXPED_ARMAZENAGEM {
    grant
        select
            on
                ZI_MM_EXPED_ARMAZENAGEM
                    where
                         ( Werks ) = aspect pfcg_auth( ZMMWERKS, WERKS, ACTVT = '03' );
                        
                        
}