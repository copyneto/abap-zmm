@EndUserText.label: 'CDS de Autorização'
@MappingRole: true
define role ZI_MM_CADASTRO_FISCAL_CABEC {
    grant
        select
            on
                ZI_MM_CADASTRO_FISCAL_CABEC
                    where
                        ( Empresa ) = aspect pfcg_auth(ZMMBUKRS, BUKRS, ACTVT = '03');
                        
}