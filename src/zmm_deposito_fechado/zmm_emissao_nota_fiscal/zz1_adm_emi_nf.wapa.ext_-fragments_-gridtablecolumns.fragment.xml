<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:table="sap.ui.table" xmlns="sap.m">                                                                                                                                                                    
    <!-- Coluna 1-->                                                                                                                                                                                                                                           
    <!-- Coluna 2-->                                                                                                                                                                                                                                           
    <table:Column hAlign="Center" width="10rem">                                                                                                                                                                                                               
        <Label text="Utilizar Quantidade"/>                                                                                                                                                                                                                    
        <table:template>                                                                                                                                                                                                                                       
            <CheckBox id="FieldUseAvailableCheckBox" selected="{UseAvailableCheckBox}" select="onChangeAvailableCheckBoxAction" enabled="{UseAvailableCheckBoxEnable}"/>                                                                                       
        </table:template>                                                                                                                                                                                                                                      
        <table:customData>                                                                                                                                                                                                                                     
            <core:CustomData key="p13nData" value="\{&quot;columnKey&quot;: &quot;UseAvailableCheckBox&quot;, &quot;columnIndex&quot;: &quot;12&quot;, &quot;leadingProperty&quot;: &quot;UseAvailableCheckBox&quot;}"/>                                       
        </table:customData>                                                                                                                                                                                                                                    
    </table:Column>                                                                                                                                                                                                                                            
    <table:Column width="15rem">                                                                                                                                                                                                                               
        <Label text="Qtd.Transferida"/>                                                                                                                                                                                                                        
        <table:template>                                                                                                                                                                                                                                       
            <StepInput id="FieldUsedStockInput" value="{UsedStock_conve}" min="0" displayValuePrecision="3" width="360px" step="1.000" largerStep="10.000" editable="{UsedStockEnable}" description="{Unit}" change="onChangeUsedStockAction"/>                
        </table:template>                                                                                                                                                                                                                                      
        <table:customData>                                                                                                                                                                                                                                     
            <core:CustomData key="p13nData" value="\{&quot;columnKey&quot;: &quot;UsedStock_conve&quot;, &quot;columnIndex&quot;: &quot;14&quot;, &quot;leadingProperty&quot;: &quot;UsedStock_conve&quot;}"/>                                                 
        </table:customData>                                                                                                                                                                                                                                    
    </table:Column>                                                                                                                                                                                                                                            
</core:FragmentDefinition>                                                                                                                                                                                                                                     