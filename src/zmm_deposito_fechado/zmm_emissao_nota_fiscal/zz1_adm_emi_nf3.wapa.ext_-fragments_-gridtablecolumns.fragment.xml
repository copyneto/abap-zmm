<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:table="sap.ui.table" xmlns="sap.m">                                                                                                                                                                    
    <!-- Coluna 1-->                                                                                                                                                                                                                                           
    <!-- Coluna 2-->                                                                                                                                                                                                                                           
    <table:Column hAlign="Center" width="10rem">                                                                                                                                                                                                               
        <Label text="Utilizar Quantidade"/>                                                                                                                                                                                                                    
        <table:template>                                                                                                                                                                                                                                       
            <CheckBox selected="{UseAvailableCheckBox}" select="onChangeAvailableCheckBoxAction" enabled="{UseAvailableCheckBoxEnable}"/>                                                                                                                      
        </table:template>                                                                                                                                                                                                                                      
        <table:customData>                                                                                                                                                                                                                                     
            <core:CustomData key="p13nData" value='\{"columnKey": "UseAvailableCheckBox", "leadingProperty": "UseAvailableCheckBox", "columnIndex": "12", "additionalProperty": "UseAvailableCheckBoxEnable"}'/>                                               
        </table:customData>                                                                                                                                                                                                                                    
    </table:Column>                                                                                                                                                                                                                                            
    <table:Column width="15rem">                                                                                                                                                                                                                               
        <Label text="Qtd.Transferida"/>                                                                                                                                                                                                                        
        <table:template>                                                                                                                                                                                                                                       
            <StepInput id="UsedStockInput" value="{UsedStock_conve}" min="0" displayValuePrecision = "3" width="360px" step="1.000" largerStep="10.000" editable="{UsedStockEnable}" description="{Unit}" change="onChangeUsedStockAction"/>                   
        </table:template>                                                                                                                                                                                                                                      
        <table:customData>                                                                                                                                                                                                                                     
            <core:CustomData key="p13nData" value='\{"columnKey": "UsedStock_conve", "leadingProperty": "UsedStock_conve", "columnIndex": "14", "additionalProperty": "UseAvailableCheckBoxEnable"}'/>                                                         
        </table:customData>                                                                                                                                                                                                                                    
    </table:Column>&nbsp;                                                                                                                                                                                                                                      
</core:FragmentDefinition>                                                                                                                                                                                                                                     